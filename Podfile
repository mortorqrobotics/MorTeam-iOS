target 'MorMap' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MorMap
  pod 'GoogleMaps'
	
  target 'MorMapTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MorMapUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
